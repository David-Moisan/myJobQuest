<div class="row">
	<div class="col-12">
		<div class="page-title-box">
			<div class="page-title-right">
				<ol class="route m-0">
					{% for route in routes %}
						<li class="route-item">
							<a href="{{ route }}">
								{{route|capitalize}}
							</a>
						</li>
					{% endfor %}
				</ol>
			</div>
			<h4 class="page-title">{{title}}</h4>
		</div>
	</div>
</div>
